#: inyoka/portal/views.py:198 inyoka/portal/views.py:312
#: inyoka/portal/views.py:370
msgid "You are already logged in."
msgstr "Du bist bereits angemeldet."

#: inyoka/portal/views.py:227
#, python-format
msgid ""
"The username “%(username)s“ was successfully registered. An email with "
"the activation key was sent to “%(email)s“."
